<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="
       http://www.springframework.org/schema/beans
       http://www.springframework.org/schema/beans/spring-beans.xsd
       http://www.springframework.org/schema/context
       http://www.springframework.org/schema/context/spring-context.xsd">


    <context:annotation-config/>

    <bean id="activityControlList" class="ru.silvestrov.timetracker.model.activitycontrollist.ActivityControlList"/>

    <bean id="renameActivityController" class="ru.silvestrov.timetracker.view.RenameActivityController"/>

    <bean id="adjustTimeController" class="ru.silvestrov.timetracker.view.AdjustTimeController"/>

    <bean id="activityControlListView" class="ru.silvestrov.timetracker.view.ActivityControlListView"/>

    <bean id="mainFrameView" class="ru.silvestrov.timetracker.view.MainFrameView" init-method="show"/>
</beans>